    "@wdio/globals": "^9.13.0",
    "@wdio/local-runner": "^9.13.0",
    "@wdio/mocha-framework": "^9.13.0",
    "@wdio/spec-reporter": "^9.13.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "react-native-svg-transformer": "^1.5.1",
    "sharp-cli": "^5.2.0",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-native": "0.81.4"
  }
}
